{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2020",
    "importsNotUsedAsValues": "error",
    "isolatedModules": true,
    "sourceMap": true,
    "types": ["svelte", "node"],
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "composite": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "baseUrl": "."
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.lib.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
